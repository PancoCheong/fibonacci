{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
	  "name": "frontend",
	  "image": "panco/fibonacci-frontend",
	  "hostname": "frontend",
	  "essential": false,
	     "memory": 128
	},
    {
	  "name": "api",
	  "image": "panco/fibonacci-api",
	  "hostname": "api",
	  "essential": false,
	  "memory": 128
	},
    {
	  "name": "worker",
	  "image": "panco/fibonacci-worker",
	  "hostname": "worker",
	  "essential": false,
	     "memory": 128
	},
    {
	  "name": "nginx",
	  "image": "panco/fibonacci-nginx",
	  "hostname": "nginx",
	  "essential": true,
	  "portMappings": [
	  {
	    "hostPort": 80,
		"containerPort": 80
	  }
	  ],
	  "links": ["frontend", "api"],
	     "memory": 128
	}
  ]
}
